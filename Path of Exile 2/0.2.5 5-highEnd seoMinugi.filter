    ItemLevel >= 82
    Class == "Body Armours"
    BaseType == "Soldier Cuirass" "Slipstrike Vest" "Vile Robe" "Death Mail" "Wolfskin Mantle" "Sleek Jacket"
    Rarity == Normal
    SetFontSize 39 #normal
    SetBorderColor 178 255 255 255 #B2FFFF
    PlayEffect Grey Temp

