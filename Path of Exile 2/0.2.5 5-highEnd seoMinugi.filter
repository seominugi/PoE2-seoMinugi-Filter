    Class == "Body Armours"
    BaseType == "Ornate Plate" "Utzaal Cuirass" "Soldier Cuirass"
    Rarity == Normal
    SetFontSize 39 #normal
    SetBackgroundColor 77 0 0 240 #4D0000

